<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>nomad</string>
  <key>ProgramArguments</key>
  <array>
    <string>/usr/local/bin/nomad</string>
    <string>agent</string>
    <string>-client</string>
    <string>-config=~/.config/nomad/nomad-client.hcl</string>
    <string>-data=/tmp/nomad/data/nomad-client/</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>StandardOutPath</key>
  <string>/tmp/nomad.stdout.log</string>
  <key>StandardErrorPath</key>
  <string>/tmp/nomad.stderr.log</string>
</dict>
</plist>
